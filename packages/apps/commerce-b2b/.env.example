# This controls which environment variables can be controlled at runtime
# through setting an environment variable prior to starting the webserver -
# csr or ssr. 
# Please use this with caution - especially note that including
# feature flags in here may cause the compiled bundle to be significantly
# larger
DYNAMIC_ENV_KEYS=GRAPHQL_ENDPOINT,GRAPHQL_BATCH

#######################
#
# APP
#
#######################

PORT=3200

# Allow SSR to be run on different port to enable simultaneous testing
SSR_PORT=3210

# Enabling this will speed up repeated builds, but may have unforeseen side-effects
# EXPERIMENTAL_BUILD_CACHE=true

# Enable Compression on the node server, may not be desired in some cases
NODE_GZIP=true

# Enable to prevent sensitive queries in SSR, useful for B2B context where queries may need user details to return correct details
SKIP_SSR_QUERIES=true

#######################
#
# GRAPHQL
#
#######################

#GRAPHQL_ENDPOINT=http://localhost:5000/graphql
GRAPHQL_ENDPOINT=https://ixl-adapter-b2b-commerce.uki-commerce-clus-382893-ed73e322e3ea12289f5ccd3b2effe14c-0000.eu-gb.containers.appdomain.cloud/graphql

# Enable to use Apollo Client Batching functionality
GRAPHQL_BATCH=false

#######################
#
# Theming
#
#######################
# A theme local to the app can be used
PROVIDERS_THEME=packages/themes/dark-theme/src/index.js
# A theme in the file system can be used
# PROVIDERS_THEME=Users/mr-tuscany/Documents/work/ixl-frontend-theme-commerce-custom/src

#######################
