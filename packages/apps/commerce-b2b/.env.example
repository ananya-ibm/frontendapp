#######################
#
# OIDC Settings
#
#######################
OIDC_PUBLIC_URL=https://localhost:3200

# Possible flows are password (default), OPENID_CONNECT__PKCE_BACK_CHANNEL or OPENID_CONNECT__PKCE_FRONT_CHANNEL
OIDC_FLOW=password

# Prefer use of cookies or tokens - valid values are token or cookie
# In case of OIDC_FLOW=OPENID_CONNECT__PKCE_BACK_CHANNEL cookies are always used
# In case of OIDC_FLOW=password tokens are always used
OIDC_AUTH_CARRIER=token



B2B_ALLOW_ORG_ADDRESS=true