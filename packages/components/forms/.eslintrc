{
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      },
      "alias": {
        "map": [
          ["renderWithTheme", "./test/renderWithTheme"],
          ["mockSiteData", "./test/mockSiteData"]
